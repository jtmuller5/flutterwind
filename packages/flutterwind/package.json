{
  "name": "flutterwind",
  "version": "1.0.0",
  "description": "React Component library modeled after Flutter Widgets. Built with Tailwind",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
